<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Mon Site
			{% endblock %}
		</title>
		<link rel="icon" type="image/png" href="/assets/img/mstile-144x144.png">
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
			<link rel="preconnect" href="https://fonts.googleapis.com">
			<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
			<link href="https://fonts.googleapis.com/css2?family=Libre+Baskerville:ital,wght@0,400;0,700;1,400&family=Playfair+Display+SC:ital,wght@0,400;0,700;0,900;1,400;1,700;1,900&family=Playfair+Display:ital,wght@0,400..900;1,400..900&family=Raleway+Dots&family=Raleway:ital,wght@0,100..900;1,100..900&display=swap" rel="stylesheet">
			<link rel="stylesheet" href="https://bootswatch.com/5/solar/bootstrap.min.css">
			<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
			<link href="{{ asset('assets/css/style.css') }}" rel="stylesheet">
		{% endblock %}
		{% block javascripts %}
			{% block importmap %}
				{{ importmap('app') }}
			{% endblock %}
		{% endblock %}
	</head>
	<body>
		<header>
			{% include "shared/_navbar.html.twig" %}
		</header>
		<main role="main">
			{% include "shared/_flash.html.twig" %}
			{% block body %}{% endblock %}
		</main>
		{% include "shared/_footer.html.twig" %}

	</body>
</html>
